for (int i = 0; i < strlen(S); i++) {
        if (S[i] == '(' || S[i] == '{' || S[i] == '[') {
            c++;
        }
        if (S[i] == ')' || S[i] == '}' || S[i] == ']') {
            d++;
        }
    }

    if (c == d) {
        cout << "good";
    }
    else {
        cout << "bad";
    }
